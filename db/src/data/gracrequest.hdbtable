COLUMN TABLE "GRACREQUEST.Header" (
     "REQID" NVARCHAR(32) NOT NULL COMMENT 'Request ID',
	 "CREATEDBY" NVARCHAR(10) COMMENT 'Created By',
	 "CREATEDAT" DATE COMMENT 'Created Date',
	 "BPROC" NVARCHAR(10) COMMENT 'Business Process',
	 PRIMARY KEY ("REQID"))
	 COMMENT 'Access Request Header'
	 	 WITH ASSOCIATIONS( JOIN "GRACREQPROVITEM.Item" AS "ITEMS" ON "REQID" = "REQID")
	 UNLOAD PRIORITY 5 AUTO MERGE