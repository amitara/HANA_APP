COLUMN TABLE "GRACREQPROVITEM.Item" (
     "ReqHeader.REQID" NVARCHAR(32) NOT NULL COMMENT 'Request ID',
	 "ITEMNUM" NVARCHAR(10) NOT NULL COMMENT 'Product ID',
	 "CONNECTOR" NVARCHAR(32) COMMENT 'System',
	 "CURRENCY" NVARCHAR(5) COMMENT 'Currency',
	 PRIMARY KEY ("ReqHeader.REQID","ITEMNUM"))
	 COMMENT 'Request Item'	 
	 WITH ASSOCIATIONS( JOIN "GRACREQUEST.Header" AS "ReqHeader" ON "ReqHeader"."REQID" = "ReqHeader.REQID")
	 UNLOAD PRIORITY 5 AUTO MERGE